// Reducers
export default function Reducer(state = {}, action = {}) {
    switch (action.type) {
        case LOAD: {
            return {
                ...state,
                loading: true,
                loaded: false,
            };
        }

        case LOAD_FAILURE: {
            return {
                ...state,
                loading: false,
                loaded: false,
                error: true,
            };
        }

        case LOAD_SUCCESS: {
            return {
                ...state,
                data: deep(action, 'data'),
                loading: true,
                loaded: false,
            };
        }

        default:
            return state;
    }
}


